{
	"name": "SimpleLDAPAuth",
	"version": "1.0",
	"author": [
		"Shiz"
	],
	"url": "https://github.com/revspace/mediawiki-SimpleLDAPAuth",
	"descriptionmsg": "simpleldapauth-desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"PluggableAuth": "7.*"
		}
	},

	"AvailableRights": [
		"ext.simpleldapauth.link"
	],
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SimpleLDAPAuth\\" : "src/"
	},
	"MessagesDirs": {
		"SimpleLDAPAuth": [
			"i18n"
		]
	},
	"APIModules": {
		"ext.simpleldapauth.unlink": {
			"class": "MediaWiki\\Extension\\SimpleLDAPAuth\\Api\\Unlink"
		}
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\SimpleLDAPAuth\\Hooks::onLoadExtensionSchemaUpdates"
	},

	"config_prefix": "SimpleLDAPAuth",
	"config": {},
	"attributes": {
		"PluggableAuth": {
			"SimpleLDAPAuth": {
				"class": "MediaWiki\\Extension\\SimpleLDAPAuth\\LDAPPluggableAuth",
				"services": [
					"AuthManager",
					"UserFactory",
					"DBLoadBalancer"
				]
			}
		}
	},
	"load_composer_autoloader" : true,
	"manifest_version": 2
}

{
	"name": "HybridLDAPAuth",
	"version": "1.0",
	"author": [
		"Shiz"
	],
	"url": "https://github.com/revspace/mediawiki-HybridLDAPAuth",
	"descriptionmsg": "ext.hybridldap.auth-desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},

	"AvailableRights": [
		"ext.hybridldap.link"
	],
	"AuthManagerAutoConfig": {
		"primaryauth": {
			"HybridLDAPAuth": {
				"class": "MediaWiki\\Extension\\HybridLDAPAuth\\Auth\\PrimaryLDAPAuthProvider",
				"services": [
					"HybridLDAPAuth.LDAPAuthManager"
				],
				"sort": 50
			}
		},
		"secondaryauth": {
			"ConfirmLink": {
				"class": "MediaWiki\\Auth\\ConfirmLinkSecondaryAuthenticationProvider",
				"sort": 50
			}
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\HybridLDAPAuth\\" : "src/"
	},
	"MessagesDirs": {
		"HybridLDAPAuth": [
			"i18n"
		]
	},
	"APIModules": {
		"ext.hybridldap.unlink": {
			"class": "MediaWiki\\Extension\\HybridLDAPAuth\\Api\\Unlink"
		}
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\HybridLDAPAuth\\Hooks::onLoadExtensionSchemaUpdates"
	},
	"ServiceWiringFiles": ["src/ServiceWiring.php"],

	"config_prefix": "HybridLDAP",
	"config": {
		"Domains": []
	},
	"attributes": {
		"PluggableAuth": {
			"HybridLDAPAuth": {
				"class": "MediaWiki\\Extension\\HybridLDAPAuth\\Auth\\PluggableLDAPAuthProvider",
				"services": [
					"AuthManager",
					"HybridLDAPAuth.LDAPAuthManager"
				]
			}
		}
	},
	"load_composer_autoloader" : true,
	"manifest_version": 2
}
